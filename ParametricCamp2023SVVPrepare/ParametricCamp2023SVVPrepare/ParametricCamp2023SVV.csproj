<Project Sdk="Microsoft.NET.Sdk">
	
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <Version>1.0</Version>
    <Title>ParametricCamp2023SVVPrepare</Title>
    <Description>Description of ParametricCamp2023SVVPrepare</Description>
    <TargetExt>.gha</TargetExt>	 
	<OutputPath>C:\Users\$(UserName)\AppData\Roaming\Grasshopper\Libraries\$(MSBuildProjectName)</OutputPath>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="GeometryGymIFC_Core" Version="0.2.15" />
    <PackageReference Include="Grasshopper" Version="7.13.21348.13001" IncludeAssets="compile;build" />
  </ItemGroup>
  
  <ItemGroup>
    <Compile Update="Bridge Generator\CrossSecCurves.cs">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
  </ItemGroup>
  
  <PropertyGroup Condition="$(Configuration) == 'Debug' AND $([MSBuild]::IsOSPlatform(Windows))">
	<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
	<StartProgram>C:\Program Files\Rhino 7\System\Rhino.exe</StartProgram>
    <StartArguments></StartArguments>
    <StartAction>Program</StartAction>
  </PropertyGroup>

</Project>